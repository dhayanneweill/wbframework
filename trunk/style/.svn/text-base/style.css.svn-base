/*****Friend Container****/
/* Friend Requests */
.requests-container {
    display: inline-block;
    width: 355px;
    min-height: 75px;
    background: #561AAD;
    margin-bottom: 10px;
    margin-left: 12px;
    padding: 10px;
    margin-top: 6px;
}
.requests-container .user-photo {
    display: inline-block;
    vertical-align: top;
    width: 50px;
    height: 50px;
    margin-left: 5px;
    margin-right: 5px;
    overflow: hidden;
}
.requests-container .user-photo img {
    width: 50px;
    height: 50px;
}
.requests-container .user-info {
    display: inline-block;
    vertical-align: top;
    width: 180px;
    overflow: hidden;
    margin-left: 5px;
}
.requests-container .user-info > div {
    margin-bottom: 5px;
    font-size: 12px;
}
.requests-container .user-info .name {
    font-size: 14px;
}
.requests-container .actions {
    display: inline-block;
    width: 100px;
}
.requests-container .actions > .button:first-child {
    margin-top: 5px;
    margin-bottom: 25px;
}

/***********Edit Account*************/
.editaccount-container {
    min-height: 200px;
}
.editprofile-header {
    color: white;
    padding: 10px 10px 5px;
}
.editprofile-header img {
    display: inline-block;
    vertical-align: middle;
}
.editprofile-header .editprofile-title {
    display: inline-block;
    vertical-align: middle;
    width: 390px;
}
/* Body */
#form-register {
    padding: 10px;
}
.editprofile-line {
    margin-top: 5px;
    margin-bottom: 5px;
    padding-top: 3px;
    padding-bottom: 3px;
    font-size: 12px;
    margin-left: 10px;
}
.editprofile-line .editprofile-label {
    display: inline-block;
    vertical-align: top;
    text-align: right;
    width: 70px;
    margin-right: 10px;
}
.editprofile-line .editprofile-input {
    display: inline-block;
    vertical-align: middle;
    width: 220px;
    font-weight: bold;
    margin-left: 10px;
    margin-right: 10px;
}
.editprofile-line .editaccount-miniform {
    width: 206px;
}
.editprofile-line .editprofile-input input[type="text"], .editprofile-line .editprofile-input input[type="password"] {
    margin: 0;
    padding: 2px;
    width: 200px;
    margin-bottom: 5px;
}
.editprofile-line .editprofile-input select {
    margin: 0;
    padding: 2px;
    width: 206px;
}
.editprofile-line .editprofile-edit {
    display: inline-block;
    vertical-align: top;
    cursor: pointer;
}
.editprofile-buttons {
    text-align: right;
}
